<%- include('partials/header') -%>
<div class="row find-and-report-form">
    <form action="/" method="POST" class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input id="input_text" type="text" data-length="10" name="crop-type">
                <label for="input_text">Crop type</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input id="input_text" type="text" data-length="10" name="market-value">
                <label for="input_text">Current market value</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s12">
                <textarea id="textarea2" class="materialize-textarea" data-length="120" name="fertilizers"></textarea>
                <label for="textarea2">Fertilizers used</label>
            </div>
        </div>
        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
        </button>
    </form>
</div>
<%- include('partials/footer') -%>



